{
  "name": "i18nCompiler",
  "version": "0.1.0",
  "description": "Compiles localization strings in a project and generates sets of js/html per each language supported in the app. Supports client side as well. Based in SlexAxton/messageformat.js",
  "main": "i18nCompiler.js",
  "scripts": {
    "test": "mocha test/bootstrap.js test/spec/**/*.js",
    "dtest": "mocha test/bootstrap.js test/spec/**/*.js --debug-brk"
  },
  "repository": {
    "type": "git",
    "url": "https://tomasruizr@github.com/tomasruizr/i18nCompiler.git"
  },
  "keywords": [
    "localization",
    "i18n",
    "i10n",
    "translation",
    "translate",
    "gulp",
    "grunt",
    "messageformat"
  ],
  "author": "Tomas Ruiz <tomasruizr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomasruizr/i18nCompiler/issues"
  },
  "homepage": "https://github.com/tomasruizr/i18nCompiler",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "fs-extra": "^0.26.7",
    "globule": "^0.2.0",
    "lodash": "^4.6.1",
    "messageformat": "^0.2.0"
  }
}
